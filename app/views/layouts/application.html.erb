<% current_user = session[:current_user_id] %>
<% logout_url = AppEngine::Users.create_logout_url('/logout') %>


<body>
<div class="wrapper col0">
  <div id="topbar">
    <div id="slidepanel">
      <div class="topbox">
        <h2>Phone Access Information</h2>
        <p>
        	To test the live application:
	<ul>
	<li>Voice & Messaging: (408) 940-5938</li>
	<li>Skype Voice: <a href="skype:+99000936 9991429707?call">+99000936 9991429707</a></li>
	<li>SIP Voice: <a href="sip:9991429707@sip.tropo.com">sip:9991429707@sip.tropo.com</a></li>
	<li>IM: openvoice@tropo.im</li>
	<li>Twitter: <a href="http://twitter.com/openvoice">@openvoice</a></li>
	</ul>
	<br/>
        </p>
        <!-- <p class="readmore"><a href="#">Continue Reading &raquo;</a></p> -->
      </div>
      <div class="topbox">
        <h2>More Info</h2>
      </div>
      <div class="topbox last">
        <h2>More Info</h2>
      </div>
      <br class="clear" />
    </div>
    <div id="loginpanel">
      <ul>
        <li class="left">
			Phone Access
		</li><li class="right" id="toggle"><a id="slideit" href="#slidepanel">Information</a><a id="closeit" style="display: none;" href="#slidepanel">Close Panel</a></li>
      </ul>
    </div>
    <br class="clear" />
  </div>
</div>
<!-- ####################################################################################################### -->
<div class="wrapper col1">
  <div id="header">
    <div id="logo">
      <h1><a href="/">MyOpenVoice</a></h1>
      <p>Open Source Phone System API</p>
    </div>
    <div id="topnav">
      <ul>
	
<% if !current_user.nil? %>	

<!-- <li><a href="/">Home</a></li> -->
<li><%= link_to "voice calls", user_voice_calls_path(current_user, @voice_calls) %></li>
<li><%= link_to "messages", user_messagings_path(current_user, @messagings) %></li>
<li><%= link_to "voicemails", user_voicemails_path(current_user, @voicemails) %></li>
<li><%= link_to "addressbook", user_contacts_path(current_user, @contacts) %></li>
<li><%= link_to "phone numbers", user_phone_numbers_path(current_user, @phone_numbers) %></li>
<li><%= link_to "call logs", user_call_logs_path(current_user) %></li>
<li><%= link_to "profile", user_profiles_path(current_user, @profiles) %></li>
<li><a href="/api">API</a></li>
<li class="last"><a href="<%= logout_url%>">logout</a></li>
		     
<% else %>
<li class="last"><a href="/profiles">Login</a></li>
		 
<% end %>
		
      </ul>
    </div>
    <br class="clear" />
  </div>
</div>

<!-- ####################################################################################################### -->
<div class="wrapper col2">
  <div id="breadcrumb">
    <ul>
      <li class="first"><p>Welcome <%= session[:nickname].capitalize %> </p></li>
