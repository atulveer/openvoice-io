      <div class='flash error'><br/><%= flash[:error] %></div>
    <% end %><% if flash[:notice] %>
      <div class='flash notice'><br/><%= flash[:notice] %></div>
    <% end %>

    <div>   
      <%= yield %>
    </div>

</div>
