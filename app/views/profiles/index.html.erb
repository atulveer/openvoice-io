<% current_user = session[:current_user_id] %>
<h1>Your Profile</h1>

<table>
  <tr>
<thead>    <th>Voice</th>
    <th>Skype</th>
    <th>Sip</th>
    <th>INum</th>
    <th>Tropo</th>
    <th>Twitter</th>
    <th>Gtalk</th>
<th colspan="1">&nbsp;</th>
</thead>  </tr>

<% if @profiles %>
<% @profiles.each do |profile| %>
  <tr>
    <td><%=h profile.voice %></td>
    <td><%=h profile.skype %></td>
    <td><%=h profile.sip %></td>
    <td><%=h profile.inum %></td>
    <td><%=h profile.tropo %></td>
    <td><%=h profile.twitter %></td>
    <td><%=h profile.gtalk %></td>
    <!-- <td><%= link_to 'Show', profile_path(profile) %></td> -->
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <!-- <td><%= link_to 'Delete', profile_path(profile), :confirm => 'Are you sure?', :method => :delete %></td> -->
  </tr>
<% end %>
<% end %>
</table>
