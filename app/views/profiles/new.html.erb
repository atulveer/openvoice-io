<% current_user = session[:current_user_id] %>
<h1>New profile</h1>

<% form_for([@profile]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :voice %><br />
    <%= f.text_field :voice %>
  </p>
  <p>
    <%= f.label :skype %><br />
    <%= f.text_field :skype %>
  </p>
  <p>
    <%= f.label :sip %><br />
    <%= f.text_field :sip %>
  </p>
  <p>
    <%= f.label :inum %><br />
    <%= f.text_field :inum %>
  </p>
  <p>
    <%= f.label :tropo %><br />
    <%= f.text_field :tropo %>
  </p>
  <p>
    <%= f.label :twitter %><br />
    <%= f.text_field :twitter %>
  </p>
  <p>
    <%= f.label :gtalk %><br />
    <%= f.text_field :gtalk %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', profiles_path %>