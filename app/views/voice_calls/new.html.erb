<% current_user = session[:current_user_id] %>
<h1>new voice call</h1>

<% form_for([@voice_call]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :to %><br />
    <%= f.text_field :to %> (including country code)
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', voice_calls_path %>